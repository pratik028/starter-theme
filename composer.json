{
	"name": "pratik028/starter-theme",
	"description": "Basic Starter theme for Block templates.",
	"keywords": [ "wordpress", "plugin" ],
	"type": "wordpress-theme",
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Pratik Deshmukh",
			"email": "pratik_deshmukh28@yahoo.com"
		}
	],
	"require": {
		"htmlburger/carbon-fields": "^3.3",
		"php": ">=8.0"
	},
	"autoload" : {
		"classmap" : [ "includes/" ]
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"composer/installers": true
		},
		"platform": {
            "php": "8.0"
        }
	}
}
